\n        <div className = \"mainAdmin\">\n            <h1>Shopping</h1>\n            <h2>Your Items</h2>\n            <div class = \"mainAdmin_buttons\">\n                <Link to='/edit-delete'>\n                        <button className=\"mainAdmin_button\">Edit</button>\n                </Link>\n                <Link to='/create'>\n                        <button className=\"mainAdmin_button\">Add</button>\n                </Link>\n            </div>\n            <div class = \"mainAdmin_items\">{adminItems}</div>\n            \n        </div>\n    )\n}\nexport default MainAdmin","/Users/kaylafang/Documents/hkp/webdev/webdev/e-commerce-app/src/components/edit&delete-page/edit-delete.js",["88"],"/Users/kaylafang/Documents/hkp/webdev/webdev/e-commerce-app/src/components/item-page/item.js",[],"/Users/kaylafang/Documents/hkp/webdev/webdev/e-commerce-app/src/components/main-user-page/main-user.js",["89"],"import React, { useState, useEffect } from 'react';\nimport { Link } from 'react-router-dom';\nimport JWT from 'jsonwebtoken';\nimport UserItemListing from \"./user-item-listing\";\nimport \"./main-user.css\";\n\nconst BASE_URL = \"https://hkp-training-teamprj.herokuapp.com\";\n\nconst MainUser = () => {\n    const [items, setItems] = useState([]);\n    const [numItemsInCart, setNumItemsInCart] = useState(0);\n    const token = localStorage.getItem('token');\n    const userId = JWT.decode(token,'Put secret in .env!!!')._id;\n\n    const getItems = () => {\n        fetch(`${BASE_URL}/items`)\n        .then(res => res.json())\n        .then(data => {\n            setItems(data.items);\n        })\n        .catch((err) => {\n            console.error(err)\n        })\n    }\n\n    const getCart = () => {\n        const requestHeaders = {\n            method: 'GET',\n            headers: { \n              'Content-Type': 'application/json',\n              'Authorization': `Bearer ${token}`\n            }\n        };\n        \n        fetch(`${BASE_URL}/users/${userId}/cart-items`, requestHeaders)\n        .then((res) => res.json())\n        .then((data) => {\n            var totalItems = 0;\n            for(var i = 0; i < data.items.length; i++) {\n                totalItems += data.items[i].quantity;\n            }\n            setNumItemsInCart(totalItems);\n        })\n        .catch((err) => {\n            console.error(err)\n        })\n    }\n\n    useEffect(() => {\n       getItems();\n    }, []);\n\n    useEffect(() => {\n        getCart();\n    }, []);\n\n    return (\n        <div id=\"user-main-container\">\n            <div id=\"user-items-page-header\">\n                <h1>Shopping</h1>\n                <Link to=\"/checkout\" id=\"cart-link\">\n                    <i id=\"cart-icon\" className=\"fa fa-shopping-cart\" style={{fontSize: \"48px\"}}>\n                        <span id=\"cart-count\">{numItemsInCart}</span>\n                    </i>\n                </Link>\n            </div>\n            <h2 id=\"user-page-title\">Products</h2>\n            <p id=\"user-page-message\">Click Items To Add To Cart</p>\n            <div id=\"user-items-container\">\n            {\n                items.map(item => {\n                    return (\n                        <UserItemListing key={item._id} \n                            id={item._id}\n                            name={item.itemname} \n                            price={item.price} \n                            imgs={item.pictures} \n                            description={item.description} \n                        />\n                    )\n                })\n            }\n            </div>\n        </div>\n    )\n}\nexport default MainUser;","/Users/kaylafang/Documents/hkp/webdev/webdev/e-commerce-app/src/components/checkout-page/checkout.js",["90"],"/Users/kaylafang/Documents/hkp/webdev/webdev/e-commerce-app/src/components/main-admin-page/admin-item.js",["91","92","93"],"import React from 'react'\nimport './main-admin.css'\n\nconst AdminItem = (props) => {